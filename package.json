    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "syntax-dynamic-import"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/deodat/react-hacker-news.git"
    },
    "keywords": [
        "react",
        "curriculum"
    ],
    "author": "DGou for The Stash House",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/deodat/react-hacker-news/issues"
    },
